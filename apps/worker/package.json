{
  "name": "@dueflow/worker",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "with-env": "dotenv -e ../../.env --",
    "dev": "pnpm run with-env tsx ./src/index.ts",
    "build": "tsc --noEmit -p ./tsconfig.json",
    "typecheck": "tsc --noEmit -p ./tsconfig.json",
    "backfill:normalize-invoice-status": "tsx ./src/backfills/normalize-invoice-status.ts",
    "backfill:seed-intelligence": "tsx ./src/backfills/seed-intelligence-snapshots.ts",
    "backfill:hydrate-workflow-versions": "tsx ./src/backfills/hydrate-workflow-versions.ts"
  }
}
