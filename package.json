    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.30.5",
    "postcss": "^8.5.3",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4.0.15",
    "tsx": "^4.20.5",
    "turbo": "^2.3.3",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.40.0"
  }
