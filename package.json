{
  "name": "dueflow",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "check": "biome check .",
    "check:unsafe": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "dev": "turbo run dev --parallel",
    "dev:web": "turbo run dev --filter=@dueflow/web",
    "dev:worker": "turbo run dev --filter=@dueflow/worker",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:mg": "pnpm run db:generate && pnpm run db:migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "preview": "turbo run build --filter=@dueflow/web && turbo run start --filter=@dueflow/web",
    "start": "turbo run start --filter=@dueflow/web",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test --filter=@dueflow/web",
    "turbo:dev": "turbo run dev --parallel",
    "turbo:build": "turbo run build",
    "turbo:typecheck": "turbo run typecheck",
    "turbo:test": "turbo run test"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.2",
    "@better-auth/expo": "^1.3.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@xyflow/react": "^12.8.6",
    "better-auth": "^1.3",
    "bullmq": "^5.58.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.5.2",
    "drizzle-orm": "^0.41.0",
    "ioredis": "^5.8.1",
    "lucide-react": "^0.574.0",
    "next": "^15.2.3",
    "postgres": "^3.4.4",
    "radix-ui": "^1.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.7.0",
    "resend": "^6.0.4",
    "server-only": "^0.0.1",
    "stripe": "^17.4.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.4.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
